@implements IDialogContentComponent

<FluentDialogBody>

    <TitleTemplate>
        My Title
    </TitleTemplate>

    <ChildContent>
        Hello Body
    </ChildContent>

    <ActionTemplate>
        <FluentButton Appearance="ButtonAppearance.Primary">One</FluentButton>
        <FluentButton>Two</FluentButton>
    </ActionTemplate>

</FluentDialogBody>

@code {
    [CascadingParameter]
    public FluentDialog? Dialog { get; set; }

    [Parameter]
    public object? Content { get; set; }

    public class MySimpleData
    {
        public string? Name { get; set; }
    }
}
