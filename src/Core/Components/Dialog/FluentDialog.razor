@namespace Microsoft.FluentUI.AspNetCore.Components
@inherits FluentComponentBase

<fluent-dialog>
    @* Content *@
    @if (Instance is not null)
    {
        <DynamicComponent Type="@Instance.ContentType" Parameters="Instance.GetParameterDictionary()" />
    }
    else
    {
        @ChildContent
    }
</fluent-dialog>
